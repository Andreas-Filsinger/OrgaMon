      'SELECT '
      '  ARTIKEL.RID,'
      '  ARTIKEL.NUMERO,'
      '  ARTIKEL.TITEL,'
      '  PRORATA.PERCENT,'
      '  ARTIKEL.VERLAG_STAT_START,'
      '  0 AUSGABEART_R,'
